local ui_options = {
	main_color = Color3.fromRGB(41, 74, 122),
	min_size = Vector2.new(500, 500),
	toggle_key = Enum.KeyCode.RightShift,
	can_resize = true,
}

do
	local imgui = game:GetService("CoreGui"):FindFirstChild("imgui")
	if imgui then imgui:Destroy() end
end

local imgui = Instance.new("ScreenGui")
local Prefabs = Instance.new("Frame")
local Windows = Instance.new("Frame")

-- Configuration initiale
imgui.Name = "imgui"
imgui.Parent = game:GetService("CoreGui")

-- Palette de couleurs ajustée pour correspondre à l'image
local Colors = {
    Background = Color3.fromRGB(20, 20, 20),    -- Noir/gris très foncé
    Accent = Color3.fromRGB(0, 120, 255),       -- Bleu vif (comme les toggles activés)
    Secondary = Color3.fromRGB(40, 40, 40),     -- Gris foncé pour les éléments secondaires
    Text = Color3.fromRGB(200, 200, 200),       -- Gris clair pour le texte
    Active = Color3.fromRGB(0, 80, 200),        -- Bleu plus foncé pour les éléments actifs
    DarkAccent = Color3.fromRGB(30, 30, 30)     -- Gris très foncé pour les fonds
}

-- === Window ===
local Window = Instance.new("ImageLabel")
Window.Name = "Window"
Window.Parent = Windows
Window.BackgroundTransparency = 1
Window.Position = UDim2.new(0, 20, 0, 20)
Window.Size
Window.Size = UDim2.new(0, 300, 0, 400)
Window.Image = "rbxassetid://2851926732"
Window.ImageColor3 = Colors.Background
Window.ScaleType = Enum.ScaleType.Slice
Window.SliceCenter = Rect.new(16, 16, 16, 16)

-- === Bar (Tabs) ===
local Bar = Instance.new("Frame")
Bar.Name = "Bar"
Bar.Parent = Window
Bar.BackgroundColor3 = Colors.DarkAccent
Bar.BorderSizePixel = 0
Bar.Position = UDim2.new(0, 0, 0, 0)
Bar.Size = UDim2.new(1, 0, 0, 30)

-- Tab Buttons
local TabButtons = Instance.new("Frame")
TabButtons.Parent = Bar
TabButtons.BackgroundTransparency = 1
TabButtons.Size = UDim2.new(1, 0, 1, 0)

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.Parent = TabButtons
UIListLayout.FillDirection = Enum.FillDirection.Horizontal
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 5)

local tabNames = {"Automation", "Killing", "Specs", "Teleports", "Rebirth", "Farming", "Calculator", "Credits"}
for i, tabName in ipairs(tabNames) do
    local TabButton = Instance.new("TextButton")
    TabButton.Parent = TabButtons
    TabButton.BackgroundTransparency = 1
    TabButton.Size = UDim2.new(0, 80, 1, 0)
    TabButton.Font = Enum.Font.GothamSemibold
    TabButton.Text = tabName
    TabButton.TextColor3 = Colors.Text
    TabButton.TextSize = 12

    local TabButtonBG = Instance.new("ImageLabel")
    TabButtonBG.Parent = TabButton
    TabButtonBG.BackgroundTransparency = 1
    TabButtonBG.Size = UDim2.new(1, 0, 1, 0)
    TabButtonBG.Image = "rbxassetid://2851929490"
    TabButtonBG.ImageColor3 = i == 2 and Colors.Active or Colors.DarkAccent -- Highlight "Killing" tab
    TabButtonBG.ScaleType = Enum.ScaleType.Slice
    TabButtonBG.SliceCenter = Rect.new(8, 8, 8, 8)
end

